<div class="company flex flex-col sm:flex-row sm:justify-between print:flex-row print:justify-between py-2 dark:text-gray-100">
  <span class="company-time sm:w-1/3 print:w-1/3"><%= company.time  %></span>
  <div class="flex flex-1 justify-between ">
    <a href="<%- company.link ? company.link : '' %> " target="blank" class="company-name hover:text-blue-400 transition-colors"><%= company.name  %></a>
    <span class="company-type <% company.type ? '' : 'hidden'  %>"><%= company.type  %></span>
  </div>
</div>
<% company.projects.forEach(project => { %>
  <%- require('./project.ejs')({project}) %>
<% }) %> 